				"          mean_vaf_percent as string,",
				"          ctdna_concentration_hge_ml as string,",
				"          normal_source as string,",
				"          notes as string,",
				"          baseline_mean_vaf_percent as string,",
				"          baseline_ctdna_concentration_hge_ml as string,",
				"          log_fold_change_mean_vaf_percent as string,",
				"          log_fold_change_ctdna_concentration_hge_ml as string,",
				"          file_name as string,",
				"          load_date as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> sourceRaw",
				"source(output(",
				"          study_name as string,",
				"          accession_id as string,",
				"          foresight_sample_id as string,",
				"          donor as string,",
				"          plasma_cfdna_concentration_ng_ml as string,",
				"          sample_type as string,",
				"          timepoint as string,",
				"          collection_date as date,",
				"          phased_variant_informed_ctdna_detected as string,",
				"          mean_vaf_percent as string,",
				"          ctdna_concentration_hge_ml as string,",
				"          normal_source as string,",
				"          notes as string,",
				"          baseline_mean_vaf_percent as string,",
				"          baseline_ctdna_concentration_hge_ml as string,",
				"          log_fold_change_mean_vaf_percent as string,",
				"          log_fold_change_ctdna_concentration_hge_ml as string,",
				"          file_name as string,",
				"          eff_start_date as timestamp,",
				"          eff_end_date as timestamp,",
				"          created_date as timestamp,",
				"          last_updated_date as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> sourceFact",
				"sourceRaw derive(eff_start_date = currentUTC('PST'),",
				"          eff_end_date = toDate('9999-12-31'),",
				"          created_date = currentUTC('PST'),",
				"          last_updated_date = currentUTC('PST'),",
				"          collection_date = toDate(collection_date,'yyyy-mm-dd')) ~> deriveDateColumns",
				"sourceFact select(mapColumn(",
				"          study_name,",
				"          tgtaccession_id = accession_id,",
				"          foresight_sample_id,",
				"          donor,",
				"          plasma_cfdna_concentration_ng_ml,",
				"          sample_type,",
				"          timepoint,",
				"          collection_date,",
				"          phased_variant_informed_ctdna_detected,",
				"          mean_vaf_percent,",
				"          ctdna_concentration_hge_ml,",
				"          normal_source,",
				"          notes,",
				"          baseline_mean_vaf_percent,",
				"          baseline_ctdna_concentration_hge_ml,",
				"          log_fold_change_mean_vaf_percent,",
				"          log_fold_change_ctdna_concentration_hge_ml,",
				"          file_name,",
				"          eff_start_date,",
				"          tgteff_end_date = eff_end_date,",
				"          created_date,",
				"          last_updated_date",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectFact",
				"selectFact filter(toString(toDate(tgteff_end_date))=='9999-12-31') ~> filterFact",
				"sourceRaw, filterFact lookup(accession_id == tgtaccession_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookupFact",
				"filterNotNullAccessionID select(mapColumn(",
				"          study_name = selectFact@study_name,",
				"          accession_id = tgtaccession_id,",
				"          foresight_sample_id = selectFact@foresight_sample_id,",
				"          donor = selectFact@donor,",
				"          plasma_cfdna_concentration_ng_ml = selectFact@plasma_cfdna_concentration_ng_ml,",
				"          sample_type = selectFact@sample_type,",
				"          timepoint = selectFact@timepoint,",
				"          collection_date = selectFact@collection_date,",
				"          phased_variant_informed_ctdna_detected = selectFact@phased_variant_informed_ctdna_detected,",
				"          mean_vaf_percent = selectFact@mean_vaf_percent,",
				"          ctdna_concentration_hge_ml = selectFact@ctdna_concentration_hge_ml,",
				"          normal_source = selectFact@normal_source,",
				"          notes = selectFact@notes,",
				"          baseline_mean_vaf_percent = selectFact@baseline_mean_vaf_percent,",
				"          baseline_ctdna_concentration_hge_ml = selectFact@baseline_ctdna_concentration_hge_ml,",
				"          log_fold_change_mean_vaf_percent = selectFact@log_fold_change_mean_vaf_percent,",
				"          log_fold_change_ctdna_concentration_hge_ml = selectFact@log_fold_change_ctdna_concentration_hge_ml,",
				"          file_name = selectFact@file_name,",
				"          eff_start_date,",
				"          eff_end_date = tgteff_end_date,",
				"          created_date,",
				"          last_updated_date",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectColumns",
				"lookupFact filter(!isNull(tgtaccession_id)) ~> filterNotNullAccessionID",
				"selectColumns derive(eff_end_date = currentUTC('PST'),",
				"          last_updated_date = currentUTC('PST')) ~> UpdateDateColumns",
				"UpdateDateColumns alterRow(updateIf(true())) ~> AlterRowUpdate",
				"deriveDateColumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 2,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          study_name,",
				"          accession_id,",
				"          foresight_sample_id,",
				"          donor,",
				"          plasma_cfdna_concentration_ng_ml,",
				"          sample_type,",
				"          timepoint,",
				"          collection_date,",
				"          phased_variant_informed_ctdna_detected,",
				"          mean_vaf_percent,",
				"          ctdna_concentration_hge_ml,",
				"          normal_source,",
				"          notes,",
				"          baseline_mean_vaf_percent,",
				"          baseline_ctdna_concentration_hge_ml,",
				"          log_fold_change_mean_vaf_percent,",
				"          log_fold_change_ctdna_concentration_hge_ml,",
				"          file_name,",
				"          eff_start_date,",
				"          eff_end_date,",
				"          created_date,",
				"          last_updated_date",
				"     )) ~> InsertNewData",
				"AlterRowUpdate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['accession_id'],",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          study_name,",
				"          accession_id,",
				"          foresight_sample_id,",
				"          donor,",
				"          plasma_cfdna_concentration_ng_ml,",
				"          sample_type,",
				"          timepoint,",
				"          collection_date,",
				"          phased_variant_informed_ctdna_detected,",
				"          mean_vaf_percent,",
				"          ctdna_concentration_hge_ml,",
				"          normal_source,",
				"          notes,",
				"          baseline_mean_vaf_percent,",
				"          baseline_ctdna_concentration_hge_ml,",
				"          log_fold_change_mean_vaf_percent,",
				"          log_fold_change_ctdna_concentration_hge_ml,",
				"          file_name,",
				"          eff_start_date,",
				"          eff_end_date,",
				"          created_date,",
				"          last_updated_date",
				"     )) ~> UpdateFact"
			]
		}
	}
