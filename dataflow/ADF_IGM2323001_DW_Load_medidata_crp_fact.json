{
	"name": "ADF_IGM2323001_DW_Load_medidata_crp_fact",
	"properties": {
		"description": "Dataflow to implement SCD Type 2 for Medidata CH Fact Table",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQL",
						"type": "DatasetReference"
					},
					"name": "sourceRaw"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQL",
						"type": "DatasetReference"
					},
					"name": "InsertNewData"
				}
			],
			"transformations": [
				{
					"name": "deriveDateColumns"
				}
			],
			"scriptLines": [
				"source(output(",
				"          project as string,",
				"          study_id as long,",
				"          subject as string,",
				"          site as string,",
				"          site_number as long,",
				"          site_group as string,",
				"          instance_name as string,",
				"          instance_repeat_number as long,",
				"          folder_name as string,",
				"          data_page_name as string,",
				"          record_position as long,",
				"          save_ts as string,",
				"          study_env_site_number as long,",
				"          crpperf as string,",
				"          crpperf_std as string,",
				"          crprsp as string,",
				"          crptpt as string,",
				"          crptpt_std as string,",
				"          crpdat as string,",
				"          crpdat_raw as string,",
				"          crpdat_int as string,",
				"          crpdat_yyyy as double,",
				"          crpdat_mm as double,",
				"          crpdat_dd as double,",
				"          crptim as string,",
				"          crprslt as double,",
				"          crprslt_raw as double,",
				"          crpdtm as string,",
				"          crpdtm_raw as string,",
				"          crpdtm_int as string,",
				"          crpdtm_yyyy as double,",
				"          crpdtm_mm as double,",
				"          crpdtm_dd as double,",
				"          crpnow as string,",
				"          crpnow_raw as string,",
				"          crpnow_int as string,",
				"          crpnow_yyyy as long,",
				"          crpnow_mm as long,",
				"          crpnow_dd as long,",
				"          crpineq as string,",
				"          crpineq_std as string,",
				"          file_name as string,",
				"          load_date as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> sourceRaw",
				"sourceRaw derive(eff_start_date = currentUTC('PST'),",
				"          eff_end_date = toDate('9999-12-31'),",
				"          created_date = currentUTC('PST'),",
				"          last_updated_date = currentUTC('PST'),",
				"          save_ts = toTimestamp(save_ts,'M/d/yyyy hh:mm:ss a'),",
				"          crpdat = toTimestamp(crpdat,'M/d/yyyy hh:mm:ss a'),",
				"          crpdat_raw = toDate(crpdat_raw,'dd MMM yyyy'),",
				"          crpdat_int = toTimestamp(crpdat_int,'M/d/yyyy h:m:s'),",
				"          crpdtm = toTimestamp(crpdtm,'M/d/yyyy hh:mm:ss a'),",
				"          crpdtm_raw = toTimestamp(crpdtm_raw,'dd MMM yyyy HH:mm'),",
				"          crpdtm_int = toTimestamp(crpdtm_int,'M/d/yyyy hh:mm:ss a'),",
				"          crpnow = toTimestamp(crpnow,'M/d/yyyy hh:mm:ss a'),",
				"          crpnow_raw = toTimestamp(crpnow_raw,'dd MMM yyyy HH:mm:ss'),",
				"          crpnow_int = toTimestamp(crpnow_int,'M/d/yyyy h:m:s a')) ~> deriveDateColumns",
				"deriveDateColumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          project,",
				"          study_id,",
				"          subject,",
				"          site,",
				"          site_number,",
				"          site_group,",
				"          instance_name,",
				"          instance_repeat_number,",
				"          folder_name,",
				"          data_page_name,",
				"          record_position,",
				"          save_ts,",
				"          study_env_site_number,",
				"          crpperf,",
				"          crpperf_std,",
				"          crprsp,",
				"          crptpt,",
				"          crptpt_std,",
				"          crpdat,",
				"          crpdat_raw,",
				"          crpdat_int,",
				"          crpdat_yyyy,",
				"          crpdat_mm,",
				"          crpdat_dd,",
				"          crptim,",
				"          crprslt,",
				"          crprslt_raw,",
				"          crpdtm,",
				"          crpdtm_raw,",
				"          crpdtm_int,",
				"          crpdtm_yyyy,",
				"          crpdtm_mm,",
				"          crpdtm_dd,",
				"          crpnow,",
				"          crpnow_raw,",
				"          crpnow_int,",
				"          crpnow_yyyy,",
				"          crpnow_mm,",
				"          crpnow_dd,",
				"          crpineq,",
				"          crpineq_std,",
				"          file_name,",
				"          eff_start_date,",
				"          eff_end_date,",
				"          created_date,",
				"          last_updated_date",
				"     )) ~> InsertNewData"
			]
		}
	}
}