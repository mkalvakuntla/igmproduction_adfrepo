{
	"name": "ADF_IGM2323001_DW_Load_medidata_exin_fact",
	"properties": {
		"description": "Dataflow to implement SCD Type 2 for medidata exin Fact Table",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQL",
						"type": "DatasetReference"
					},
					"name": "sourceRaw"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQL",
						"type": "DatasetReference"
					},
					"name": "InsertNewData"
				}
			],
			"transformations": [
				{
					"name": "deriveDateColumns"
				}
			],
			"scriptLines": [
				"source(output(",
				"          project as string,",
				"          study_id as string,",
				"          subject as string,",
				"          site as string,",
				"          site_number as string,",
				"          site_group as string,",
				"          instance_name as string,",
				"          instance_repeat_number as string,",
				"          folder_name as string,",
				"          data_page_name as string,",
				"          record_position as string,",
				"          save_ts as string,",
				"          study_env_site_number as string,",
				"          exinperf as string,",
				"          exinperf_std as string,",
				"          exincyc as string,",
				"          exincyc_std as string,",
				"          exinday as string,",
				"          exinday_raw as string,",
				"          exinrsp as string,",
				"          exinrsp_std as string,",
				"          exinae1 as string,",
				"          exinae1_std as string,",
				"          exinosp as string,",
				"          exindat as string,",
				"          exindat_raw as string,",
				"          exindat_int as string,",
				"          exindat_yyyy as string,",
				"          exindat_mm as string,",
				"          exindat_dd as string,",
				"          exinentm as string,",
				"          exinrstm as string,",
				"          exinsdtm as string,",
				"          exinsdtm_raw as string,",
				"          exinsdtm_int as string,",
				"          exinsdtm_yyyy as string,",
				"          exinsdtm_mm as string,",
				"          exinsdtm_dd as string,",
				"          exinrdtm as string,",
				"          exinrdtm_raw as string,",
				"          exinrdtm_int as string,",
				"          exinrdtm_yyyy as string,",
				"          exinrdtm_mm as string,",
				"          exinrdtm_dd as string,",
				"          exinnow as string,",
				"          exinnow_raw as string,",
				"          exinnow_int as string,",
				"          exinnow_yyyy as string,",
				"          exinnow_mm as string,",
				"          exinnow_dd as string,",
				"          exinrsyn as string,",
				"          exinrsyn_std as string,",
				"          exinrtrt as string,",
				"          exinrtrt_std as string,",
				"          file_name as string,",
				"          load_date as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> sourceRaw",
				"sourceRaw derive(eff_start_date = currentUTC('PST'),",
				"          eff_end_date = toDate('9999-12-31'),",
				"          created_date = currentUTC('PST'),",
				"          last_updated_date = currentUTC('PST'),",
				"          save_ts = toTimestamp(save_ts, 'M/dd/yyyy h:m:s a'),",
				"          exindat = toTimestamp(exindat, 'M/dd/yyyy h:m:s a'),",
				"          exindat_raw = toDate(exindat_raw, 'dd MMM yyyy'),",
				"          exindat_int = toTimestamp(exindat_int, 'M/dd/yyyy h:m:s a'),",
				"          exinsdtm = toTimestamp(exinsdtm, 'M/dd/yyyy h:m:s a'),",
				"          exinsdtm_raw = toTimestamp(exinsdtm_raw, 'dd MMM yyyy HH:mm'),",
				"          exinsdtm_int = toTimestamp(exinsdtm_int, 'M/dd/yyyy h:m:s a'),",
				"          exinrdtm = toTimestamp(exinrdtm_int, 'M/dd/yyyy h:m:s a'),",
				"          exinrdtm_raw = toTimestamp(exinrdtm_int, 'M/dd/yyyy h:m:s a'),",
				"          exinrdtm_int = toTimestamp(exinrdtm_int, 'M/dd/yyyy h:m:s a'),",
				"          exinnow = toTimestamp(exinnow_int, 'M/dd/yyyy h:m:s a'),",
				"          exinnow_raw = toTimestamp(exinnow_int, 'M/dd/yyyy h:m:s a'),",
				"          exinnow_int = toTimestamp(exinnow_int, 'M/dd/yyyy h:m:s a')) ~> deriveDateColumns",
				"deriveDateColumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          project,",
				"          study_id,",
				"          subject,",
				"          site,",
				"          site_number,",
				"          site_group,",
				"          instance_name,",
				"          instance_repeat_number,",
				"          folder_name,",
				"          data_page_name,",
				"          record_position,",
				"          save_ts,",
				"          study_env_site_number,",
				"          exinperf,",
				"          exinperf_std,",
				"          exincyc,",
				"          exincyc_std,",
				"          exinday,",
				"          exinday_raw,",
				"          exinrsp,",
				"          exinrsp_std,",
				"          exinae1,",
				"          exinae1_std,",
				"          exinosp,",
				"          exindat,",
				"          exindat_raw,",
				"          exindat_int,",
				"          exindat_yyyy,",
				"          exindat_mm,",
				"          exindat_dd,",
				"          exinentm,",
				"          exinrstm,",
				"          exinsdtm,",
				"          exinsdtm_raw,",
				"          exinsdtm_int,",
				"          exinsdtm_yyyy,",
				"          exinsdtm_mm,",
				"          exinsdtm_dd,",
				"          exinrdtm,",
				"          exinrdtm_raw,",
				"          exinrdtm_int,",
				"          exinrdtm_yyyy,",
				"          exinrdtm_mm,",
				"          exinrdtm_dd,",
				"          exinnow,",
				"          exinnow_raw,",
				"          exinnow_int,",
				"          exinnow_yyyy,",
				"          exinnow_mm,",
				"          exinnow_dd,",
				"          exinrsyn,",
				"          exinrsyn_std,",
				"          exinrtrt,",
				"          exinrtrt_std,",
				"          file_name,",
				"          load_date,",
				"          eff_start_date,",
				"          eff_end_date,",
				"          created_date,",
				"          last_updated_date",
				"     )) ~> InsertNewData"
			]
		}
	}
}